- @articles.each do |article|

  %h1.article-title
    = link_to article.show_title, "/article/#{article.title}", true

  .article= article.get_content

  = partial :stamp, :locals => {:article => article}

  %hr

- if @articles.length > 0

  #activity-more.more
    %a{onclick: "window.location = './list'"} < Click Me to see More >

    - from = @articles[-1].id
    :coffeescript
      $ ->    
          $('#activity-more').appear ()->
              more(#{from})

